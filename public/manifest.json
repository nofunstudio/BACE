{
  "name": "R3F Devtool",
  "description": "Inspect in 3D",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [ "contextMenus", "tabs", "webNavigation" ],
  "devtools_page": "devtools.html",
  "background": {
    "page": "./background.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./dist/contentscript.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "/assets/R3F.png"
  },
  "web_accessible_resources": [
    "../src/content/*.ts"
  ]
}