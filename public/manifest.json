{
  "name": "R3F Devtool",
  "description": "Inspect in 3D",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [ "contextMenus", "tabs" ],
  "background": {
    "service_worker": "/dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/dist/contentbridge.js", "/dist/appelement.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/assets/R3F.png",
      "32": "/assets/R3F.png"
    },
    "default_title": "Three.JS DevTool"
  },
  "icons": {
    "128": "/assets/R3F.png"
  },
  "web_accessible_resources": [{
    "resources": ["/dist/injection.js"],
    "matches": ["<all_urls>"]
  }]
}